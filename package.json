{
  "name": "mymovies",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://wolfgangnelli.github.io/movieApp/",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-modal": "^3.14.3",
    "react-reveal": "^1.2.2",
    "react-scripts": "3.4.0"
  },
  "scripts": {
    "build:css": "postcss src/styles/main.css --output src/main.css --watch",
    "watch:css": "postcss src/styles/main.css --output src/main.css --watch --verbose",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.7.4",
    "gh-pages": "^2.2.0",
    "postcss-cli": "^7.1.0",
    "postcss-import": "^12.0.1",
    "tailwindcss": "^1.2.0"
  }
}
